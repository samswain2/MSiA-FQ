knitr::opts_chunk$set(echo = TRUE)
auto = read.csv('Auto.csv', na.strings = 'NA')
fit_1 = lm(mpg ~ cylinders + displacement +
weight + origin + year +
I(year^2), data=auto)
summary(fit_1)
library(car)
vif(fit_1)
fit_2 = lm(mpg ~ cylinders + displacement +
origin + year + I(year^2),
data=auto)
summary(fit_2)
set.seed(1)
x = runif(500, min=0, max=5)
delta = rnorm(500)
e = rnorm(500)
y = x + delta
w = 4 + 2*x + 3*y + e
df = data.frame(y, x, w)
cor(df)
summary(df)
print("Standard Deviation Below (y, x, w)")
print(c(sd(y), sd(x), sd(w)))
fit_4c = lm(y ~ x)
summary(fit_4c)
confint(fit_4c)
fit_4d = lm(y ~ x + w)
summary(fit_4d)
confint(fit_4d)
